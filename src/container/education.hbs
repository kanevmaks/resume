<div>
    {{#with main}}
    <main>
        <section>
            {{> title big=true data="My education"}}
            <ul>
                {{#each educations}}
                <li>
                {{name}}
                
            {{#if isEnd}} (End) {{/if}}
                {{#unless isEnd includeZero=falce}} (ще в процесі) {{/unless}}
            {{#unless isEnd }}
                {{#if isEnd includeZero=true}}
                (в процесі)
                {{/if}}
            {{/unless}}
                </li>
                {{/each}}
            </ul>
        </section>

         <section>
            {{>title big=false data="My certificates"}}
            <ul class="px-5">
                {{#each certificates }}
                <li>
                    {{name}} ({{id}})
                </li>
                {{/each}}
            </ul>
        </section>
    </main>
    {{/with}}
</div>